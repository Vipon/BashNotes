{
	"languageserver": {
		"haskell": {
		"command": "hie-wrapper",
		"args": ["--lsp"],
		"rootPatterns": [
			"*.cabal",
			"stack.yaml",
			"cabal.project"
		],
		"filetypes": [
			"hs",
			"lhs",
			"haskell"
		],
		"initializationOptions": {
			"languageServerHaskell": {
				"hlintOn": true
			}
		}
		}
	}
}

